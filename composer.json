{
    "name": "cooperaj/bolt-twitter",
    "description": "Provides access to twitter and gives you the tools to show feeds, searches and hashtags",
    "type": "bolt-extension",
    "keywords" : [
        "twitter",
        "social media"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Cooper", 
            "email": "adam@networkpie.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Cooperaj\\Twitter\\": "src"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Cooperaj\\Twitter\\TwitterExtension"
    }
}
