{
    "name": "cooperaj/bolt-twitter",
    "description": "Provides access to twitter and gives you the tools to show feeds, searches and hashtags",
    "type": "bolt-extension",
    "require": {
        "bolt/bolt": ">=2.0.0,<3.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Cooper", 
            "email": "adam@networkpie.co.uk"
        }
    ],
    "autoload": {
        "files": [
            "init.php"
        ],
        "psr-4": {
            "Bolt\\Extension\\Cooperaj\\Twitter\\": [
                "",
                "src"
            ],
            "Endroid\\Twitter\\": "lib/endroid/twitter/src/Endroid/Twitter",
            "Buzz\\": "lib/kriswallsmith/buzz/lib/Buzz"
        }
    },
    "extra": {
        "bolt-assets": "assets",
        "branch-alias" : {
            "dev-master" : "1.0.*-dev"
        }
    }
}
